package leet;

public class Leet868 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Leet868 obj = new Leet868();
		int n = 22;
		System.out.println(obj.binaryGap(n));
	}

	public int binaryGap(int n) {
		String bin = Integer.toBinaryString(n);
		System.out.println(bin);
		return 0;
    }
}
