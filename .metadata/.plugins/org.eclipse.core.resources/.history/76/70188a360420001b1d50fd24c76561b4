package Java_3;

class Rodent0{
	public String name="Rodent";
	public void show() {
		System.out.println(name);
	}
}

class Mouse extends Rodent0{
	public String hisname="Mouse";
	public Mouse(){
		super.name=hisname;
	}
	
	public void show() {
		System.out.println(hisname);
	}
}

public class Rodent {

	public static void main(String argc[]) {
	
		Rodent0 Mouse=new Mouse();
		Mouse.show();
}
}

